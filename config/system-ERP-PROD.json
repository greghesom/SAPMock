{
  "systemId": "ERP-PROD",
  "name": "SAP ERP Production Environment",
  "type": "ERP",
  "enabled": true,
  "connectionParameters": {
    "server": "sap-erp-prod.company.com",
    "port": "8000",
    "client": "200",
    "language": "EN"
  },
  "modules": [
    {
      "moduleId": "MM",
      "name": "Materials Management",
      "systemId": "ERP-PROD",
      "enabled": true,
      "configFile": "module-ERP-PROD-MM.json",
      "endpoints": [
        {
          "path": "/sap/opu/rest/mm/materials",
          "method": "GET",
          "requestType": "object",
          "responseType": "object",
          "handlerType": "MaterialsManagementHandler",
          "enabled": true
        },
        {
          "path": "/sap/opu/rest/mm/materials/{id}",
          "method": "GET",
          "requestType": "object",
          "responseType": "object",
          "handlerType": "MaterialsManagementHandler",
          "enabled": true
        }
      ]
    },
    {
      "moduleId": "SD",
      "name": "Sales & Distribution",
      "systemId": "ERP-PROD",
      "enabled": true,
      "configFile": "module-ERP-PROD-SD.json",
      "endpoints": [
        {
          "path": "/sap/opu/rest/sd/customers",
          "method": "GET",
          "requestType": "object",
          "responseType": "object",
          "handlerType": "SalesDistributionHandler",
          "enabled": true
        },
        {
          "path": "/sap/opu/rest/sd/sales-orders",
          "method": "GET",
          "requestType": "object",
          "responseType": "object",
          "handlerType": "SalesDistributionHandler",
          "enabled": true
        }
      ]
    },
    {
      "moduleId": "FI",
      "name": "Financial Accounting",
      "systemId": "ERP-PROD",
      "enabled": true,
      "configFile": "module-ERP-PROD-FI.json",
      "endpoints": [
        {
          "path": "/sap/opu/rest/fi/general-ledger",
          "method": "GET",
          "requestType": "object",
          "responseType": "object",
          "handlerType": "DefaultHandler",
          "enabled": true
        }
      ]
    }
  ]
}